{
    // don't use the local cargo index, instead use the up-to-date one on the internet
    "crates.useLocalCargoIndex": false,
    "cSpell.words": [
        "ascii",
        "axum",
        "binaryen",
        "Bincode",
        "bindgen",
        "brotli",
        "concatcp",
        "createdb",
        "datetime",
        "favicons",
        "gloo",
        "invalidities",
        "justfile",
        "leptos",
        "Leptos",
        "mailchecker",
        "marzichat",
        "oneshot",
        "openssl",
        "postgres",
        "postgresql",
        "precompressed",
        "Redb",
        "rustc",
        "sqlx",
        "Stylesheet",
        "systemstat",
        "tarpc",
        "thiserror",
        "tokio",
        "uuid",
        "watchexec",
        "zeroize",
        "zxcvbn"
    ],
    "cSpell.language": "en,es,de,fr",
    "rust-analyzer.linkedProjects": [
        "./Cargo.toml"
    ],
    "rust-analyzer.check.overrideCommand": [
        "just",
        "rust-analyzer-check"
    ],
    "rust-analyzer.cargo.extraEnv": {
        // doen't require a value, just needs to be present for rust analyzer to not complain
        // The actual value is defined in the justfile.
        "CSS_FILE_NAME": "",
    },
    "rust-analyzer.check.allTargets": false,
    "rust-analyzer.showUnlinkedFileNotification": false
}